{
  "manifest_version": 3,
  "name": "WebauthN Tracer",
  "version": "1.0.0",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://localhost/*"],
      "js": ["content.js"],
      "world": "MAIN",
      "run_at": "document_start"
    }
  ],
 "icons": {
    "16": "fido16.png",
    "32": "fido32.png",
    "48": "fido48.png",
    "128": "fido128.png"
  }
}
